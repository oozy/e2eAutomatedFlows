{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"program": "${workspaceRoot}/lib/main.js",
			"protocol": "inspector",
			"runtimeArgs": ["--harmony", "--no-deprecation"],
			"outFiles": ["${workspaceRoot}/lib/**/*.js"],
			"skipFiles": ["${workspaceRoot}/node_modules/**/*.js"],
			"smartStep": true,
			"trace": true,
			"console": "integratedTerminal"
		}
	]
}
